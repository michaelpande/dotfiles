"i3lock -c 395c69 -i ~/images/lockscreen.png"
  Pause

"gnome-session quit"
  super + shift + e


"amixer -q set Master 5dB+ unmute"
  XF86AudioRaiseVolume

"amixer -q set Master 5dB- unmute"
  XF86AudioLowerVolume

"amixer -q set Master toggle"
  XF86AudioMute

"xdotool mousemove_relative 0 -15"
  Mod4 + Super_L + Alt + k

"xdotool mousemove_relative 0 15"
  Mod4 + Super_L + Alt + j

"xdotool mousemove_relative -- -15 0"
  Mod4 + Super_L + Alt + h

"xdotool mousemove_relative 15 0"
  Mod4 + Super_L + Alt + l

"xdotool click 1"
  Mod4 + Super_L + Alt + n

"xdotool click 2"
  Mod4 + Super_L + Alt + m



